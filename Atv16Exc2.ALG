Algoritmo "Atv16Exc2"

Var

   n1,n2:real

Inicio

   n1:=1
   repita
      se (n1%2=1) entao
         escreval (n1)
      fimse
      n1:=n1+1
   ate (n1>100)

   escreval ()
   escreval ()
   escreval ()

   n2:=100
   repita
      se (n2%2=1) entao
         escreval (n2)
      fimse
      n2:=n2-1
   ate (n2<1)

Fimalgoritmo